    "sass": "1.69.5",
    "sass-loader": "13.3.2",
    "storybook": "7.6.6",
    "stylelint": "16.0.2",
    "stylelint-config-standard": "35.0.0",
    "typescript": "5.2.2",
    "vite": "5.0.8",
    "vite-tsconfig-paths": "4.2.2",
    "vitest": "1.1.0"
